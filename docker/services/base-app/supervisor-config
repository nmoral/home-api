[program:amqp]
command=php /app/bin/console messenger:consume amqp
user=www-data
numprocs=1
autostart=true
autorestart=true
startretries=5
startsecs=0
process_name=%(program_name)s_%(process_num)02d
directory=/app
priority=200
stdout_logfile=var/log/messenger.log
stdout_logfile_maxbytes=10MB
stdout_logfile_backups=2
stderr_logfile=var/log/messenger.log
stderr_logfile_maxbytes=10MB
stderr_logfile_backups=2

